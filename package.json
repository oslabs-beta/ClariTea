    "css-loader": "^6.5.1",
    "electron": "^16.0.4",
    "electron-devtools-installer": "^3.2.0",
    "electron-packager": "^15.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.2.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "webpack": "^5.65.0",
